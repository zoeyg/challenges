<html>
    <header>
        <title>Competitive Situation</title>
        <style>
            body {
                height: 100%;
                margin: 0;
            }

            .container {
                height: 100%;
                padding: 0;
                margin: 0;
                display: flex;
                align-items: center;
                justify-content: center;
            }

            .row {
                width: auto;
            }

            .item {
                padding: 5px;
                margin: 10px;
                line-height: 20px;
                text-align: center;
            }
        </style>
        <script>
            document.addEventListener("DOMContentLoaded", function() {
                var plugins = window.plugins || {};
                for(var i = 0; i < plugins.length; i++) {
					var se = document.createElement('script');
                    se.src = plugins[i];
                    document.body.appendChild(se);
				}
            });
        </script>
    </header>
    <body>
		<div class="container">
			<div class="row">
				<div class="item">
					<%- report %>
				</div>
			</div>
        </div>
    </body>
</html>